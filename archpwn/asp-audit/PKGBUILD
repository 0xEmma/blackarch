# TODO: make package function

pkgname=asp-audit
pkgver=v2
pkgrel=1
pkgdesc="Look for common misconfigurations and information leaks in ASP.NET applications."
url="http://michaeldaw.org/projects/asp-auditor-v2/"
groups=('blackarch' 'blackarch-web-apps')
license=('GPL')
depends=('perl')
arch=(any)
source=(http://dl.packetstormsecurity.net/UNIX/scanners/asp-audit-${pkgver}.tar.gz)
md5sums=('eb2e18829326115db3ebccf76b97a127')

build() {
  cd "$srcdir"
  install -Dm755 asp-audit-latest.pl $pkgdir/usr/bin/asp-audit
}

