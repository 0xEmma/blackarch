# TODO: make package function

pkgname=genlist
pkgver=latest
pkgrel=2
pkgdesc="generates lists of IP addresses."
groups=('blackarch' 'blackarch-misc')
depends=('glibc')
license=('GPL')
arch=(i686 x86_64)
source=(http://packetstormsecurity.org/UNIX/scanners/genlist.c)
md5sums=('408e23d5bfbd10f75a35d884f3876be1')

build() {
  cd "$srcdir"
  gcc $CFLAGS genlist.c -o genlist
  install -Dm755 genlist $pkgdir/usr/bin/genlist
}

