pkgname=sqlmap
pkgver=1.0
pkgrel=1
pkgdesc="automatic sql injection and database takeover tool"
arch=("$CARCH")
groups=('blackarch-exploitation' 'blackarch-all')
url=('http://sqlmap.org')
license=('GPL')
makedepends=('git')
depends=('python2')
md5sums=('')
options=('!strip')

_gitrepo=('https://github.com/sqlmapproject/sqlmap')

package() {
  git clone ${_gitrepo} ${pkgname}

  cd "${srcdir}"/${pkgname}
}
